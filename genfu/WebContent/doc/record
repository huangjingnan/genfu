create table user_info (
        user_id bigint not null,
        upper_user_ID bigint,
        user_name varchar(255),
        user_code varchar(255),
        user_password varchar(255),
        user_CREATE_DATE timestamp,
        user_DESCRIPTION varchar(255),
        user_EFF_DATE timestamp,
        user_EXP_DATE timestamp,
        user_FLAG varchar(255),
        user_OTHERS varchar(255),
        user_UPDATE_DATE timestamp,
        district_id bigint,
        department_id bigint,
        primary key (user_ID)
    )

    
    火狐浏览器支持window.event
    
    <script type="text/javascript">
        if(isFirefox=navigator.userAgent.indexOf("Firefox")>0) 
        {        
            var _E = function(){
                var c=_E.caller; 
                while(c.caller)c=c.caller; 
                return c.arguments[0]
            };        
            __defineGetter__("event", _E); 
        }
    </script>

